<h1>Update exam</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: @exam, local: true) do |f| %>
      <%= render 'shared/error_messages', resource: f.object %>

      <%= f.label :Exam %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
